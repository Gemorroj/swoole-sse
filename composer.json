{
    "type": "project",
    "license": "MIT",
    "name": "gemorroj/swoole-sse",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "swoole/ide-helper": "@dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
